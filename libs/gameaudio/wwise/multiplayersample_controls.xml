<ATLConfig atl_name="multiplayersample_controls">
	<AudioTriggers>
		<ATLTrigger atl_name="Stop_Sound_Gun_Firing">
			<WwiseEvent wwise_name="Stop_Sound_Gun_Firing"/>
		</ATLTrigger>
		<ATLTrigger atl_name="Play_Sound_Gun_Firing">
			<WwiseEvent wwise_name="Play_Sound_Gun_Firing"/>
		</ATLTrigger>
	</AudioTriggers>
	<AudioPreloads>
		<ATLPreloadRequest atl_name="multiplayersample_soundbank" atl_type="AutoLoad">
			<WwiseFile wwise_name="multiplayersample_soundbank.bnk"/>
		</ATLPreloadRequest>
	</AudioPreloads>
</ATLConfig>

